        <mxCell id="E0t1wR7pqK-8ebcjUuFq-43" value="Eg: personal skill" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" visible="0" parent="1">
