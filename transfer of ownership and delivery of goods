        <mxCell id="PSnf_3OtOLj86w0sUEU5-193" value="Sale of &lt;br&gt;un-ascertained goods&lt;br&gt;&amp;nbsp;by description&lt;br&gt;&amp;nbsp;[Section 23(1)]" style="swimlane;startSize=80;fontFamily=Helvetica;fontSize=12;fontColor=#f0f0f0;align=center;strokeColor=#f0f0f0;fillColor=#2a2a2a;html=1;" parent="1" vertex="1" collapsed="1">
