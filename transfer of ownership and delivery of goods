        <mxCell id="PSnf_3OtOLj86w0sUEU5-177" value="Specific goods in a &lt;br&gt;deliverable state &lt;br&gt;(Section 20)" style="swimlane;startSize=80;fontFamily=Helvetica;fontSize=12;fontColor=#f0f0f0;align=center;strokeColor=#f0f0f0;fillColor=#2a2a2a;html=1;" parent="1" vertex="1" collapsed="1">
